<div class="fluid-container">
  <div class="item-title">
    <h1>
      <%= @box.title %>
    </h1>
  </div>
  <div class="iframe">
    <%= @iframe %>
  </div>
  <div class="box-content">
    <%= Rinku.auto_link(@box.content, :all, 'target="_blank"').html_safe %>
  </div>
  <%= link_to "Go back to Map", boxes_path, class: "btn btn-primary btn-lg btn-block button-map" %>
  <%= cl_image_tag("fpuxjteac8gur5ih28eg.jpg", width: 400, height: 300, crop: :fill) %>
</div>
